{
  "cli": {
    "version": ">= 3.14.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://africa-games-app.online/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://africa-games-app.online/api"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://africa-games-app.online/api"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://africa-games-app.online/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
